{
  "manifest_version": 3,
  "short_name": "Tabme",
  "name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "version": "1.42.0",
  "author": "gettabme@gmail.com",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "key": "",
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_icon": "icon_48.png",
    "default_title": "__MSG_extDefaultTitle__",
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "history",
    "tabs"
  ],
  "optional_host_permissions": [
    "<all_urls>"
  ]
}
