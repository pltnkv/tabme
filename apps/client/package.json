{
  "name": "tabme-chrome-extension",
  "version": "1.0.0",
  "description": "tabme-chrome-extension",
  "scripts": {
    "watch": "webpack --config webpack/webpack.dev.js --watch",
    "watch2": "webpack --config webpack/webpack.dev.js --watch --env BUILD_TYPE=overrideless",
    "build": "npm run generate-api && webpack --config webpack/webpack.prod.js",
    "publish": "node webpack/publish.js",
    "publish-patch": "node webpack/publish.js --patch",
    "clean": "rimraf dist",
    "test": "npx jest",
    "style": "prettier --write \"src/**/*.{ts,tsx}\"",
    "fetch-openapi-file": "curl -s http://localhost:3000/openapi.json > ../server/openapi.json",
    "update-schema": "openapi-typescript ../server/openapi.json -o src/api/schema.ts",
    "generate-api": "npm run fetch-openapi-file && npm run update-schema"
  },
  "author": "",
  "license": "custom",
  "dependencies": {
    "@types/react-transition-group": "^4.4.1",
    "archiver": "^7.0.1",
    "cloc": "^2.4.0-cloc",
    "fs-extra": "^11.2.0",
    "mixpanel-browser": "^2.60.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@types/chrome": "^0.0.100",
    "@types/jquery": "^3.3.33",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "copy-webpack-plugin": "^6.4.1",
    "dotenv-webpack": "^8.1.0",
    "glob": "^7.1.6",
    "openapi-fetch": "^0.14.0",
    "openapi-typescript": "^7.8.0",
    "react-switch": "^7.0.0",
    "swagger2openapi": "^7.0.8",
    "ts-loader": "^6.2.1",
    "webpack": "^5.90.1",
    "webpack-cli": "^4.0.0",
    "webpack-merge": "~4.2.2"
  }
}
