{
  "manifest_version": 3,
  "short_name": "Tabme",
  "name": "Tabme — Organize Tabs and Bookmarks. Version without newtab",
  "description": "Boost productivity — Turn chaotic browsing into a structured experience.",
  "version": "1.29.0",
  "author": "pltnkv0+tabme@gmail.com",
  "icons": {
    "16": "icon_16.png",
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "key": "",
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_icon": "icon_48.png",
    "default_title": "Open Tabme"
  },
  "permissions": [
    "bookmarks",
    "storage",
    "history",
    "tabs"
  ],
  "optional_host_permissions": [
    "<all_urls>"
  ]
}
